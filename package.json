    "googleapis": "^134.0.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.3",
    "ws": "^8.16.0"
  },
  "jest": {
    "preset": "./jest.config.js"
  },
  "engines": {
    "node": ">=18.0.0"
  }
